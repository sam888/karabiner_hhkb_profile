{
    "title": "Change profile to MyProfile",
    "rules": [
    {
        "description": "Press right_command + right_option + right_shift  to enable MyProfile",
        "manipulators": [
        {
            "from":
            {
                "key_code": "right_shift",
                "modifiers":
                {
                    "mandatory": [
                        "right_option",
                        "right_command"
                    ]

                }
            },
            "to_after_key_up": [
            {
                "shell_command": "afplay /System/Library/Sounds/Purr.aiff"

            }],
            "to": [
            {
                "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'MyProfile'"
            }],
            "type": "basic"
        },
        {
            "from":
            {
                "key_code": "left_shift",
                "modifiers":
                {
                    "mandatory": [
                        "left_control",
                        "left_command"
                    ]

                }
            },
            "to_after_key_up": [
            {
                "shell_command": "afplay /System/Library/Sounds/Purr.aiff"

            }],
            "to": [
            {
                "shell_command": "'/Library/Application Support/org.pqrs/Karabiner-Elements/bin/karabiner_cli' --select-profile 'MyProfile'"
                
            }],
            "type": "basic"
        }]
    }]
}