        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "f1"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "optional": ["any"]
            }
          },
